@{
  Layout = "_Layout";
}

@using NewsGator.Models
@model List<NewsGator.Models.NewsApiArticle>

<ol>
  @foreach (NewsApiArticle article in Model)
  {
    <ul>
      @* <li>Source: @article.Source</li> *@
      <li>Author: @article.Author</li>
      <li>Title: @article.Title</li>
      <li>Description: @article.Description</li>
      <li>Url: @article.url</li>
    </ul>
  }
</ol>